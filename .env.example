# Discord webhook URL for logging conversations
DISCORD_WEBHOOK_URL=your_discord_webhook_url_here

# Model configuration
# You can use any Hugging Face model that supports text generation
MODEL_NAME=TinyLlama/TinyLlama-1.1B-Chat-v1.0
# Alternative models:
# MODEL_NAME=mistralai/Mistral-7B-Instruct-v0.2
# MODEL_NAME=TheBloke/Llama-2-7B-Chat-GGUF

# Generation parameters
MAX_LENGTH=512
TEMPERATURE=0.7
TOP_P=0.9
REPETITION_PENALTY=1.1

# Hardware settings
USE_GPU=true
NUM_THREADS=4 